{
  "$schema": "https://json.schemastore.org/eslintrc",
  "root": true,
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  },
  "settings": {
    "next": {
      "rootDir": ["./"]
    },
    "react": {
      "version": "detect"
    }
  },
  "parserOptions": {
    "babelOptions": {
      "presets": [
        ["next/babel"]
      ]
    }
  }
}
